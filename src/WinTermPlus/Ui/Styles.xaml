<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TabButtonFgInactive" Color="{StaticResource 	MahApps.Brushes.IdealForeground}" />
    <SolidColorBrush x:Key="TabButtonBgActive" Color="{StaticResource 	MahApps.Colors.AccentBase}" />
    <SolidColorBrush x:Key="TabButtonBgHighlighted" Color="{StaticResource MahApps.Colors.Accent}" />
    <SolidColorBrush x:Key="TabButtonBgInactive" Color="{StaticResource 	MahApps.Colors.Accent2}" />
    <SolidColorBrush x:Key="TabButtonBgDisabled" Color="{StaticResource 	MahApps.Colors.Accent3}" />
</ResourceDictionary>
